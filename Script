task.wait(0.25)
game:GetService("TweenService"):Create(UI.setting.ScrollingFrame,TweenInfo.new(4,Enum.EasingStyle.Sine),{CanvasPosition = Vector2.new(0,662)}):Play()
task.wait(4)
UI.setting.ScrollingFrame.ScrollingEnabled = true
end)
end
