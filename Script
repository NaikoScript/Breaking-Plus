local Tween2 = game:GetService("TweenService"):Create(SettingButton,TweenInfo.new(0.75,Enum.EasingStyle.Linear),{ImageColor3 = Color3.fromRGB(255, 255, 255)})
Tween:Play()
local Connection
local OpenedSettings = false
local Connection2
Connection2 = Tween2.Completed:Connect(function()
if OpenedSettings == false then
Tween:Play()
else
Connection2:Disconnect()
end
end)
Connection = Tween.Completed:Connect(function()
if OpenedSettings == false then
Tween2:Play()
else
Connection2:Disconnect()
end
end)
